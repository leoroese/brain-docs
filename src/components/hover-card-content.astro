
---
interface Props {
  path: string
}
import { getEntry } from "astro:content"

const entry = await getEntry("docs", Astro.props.path)
let Content = null

if (entry != null) {
  const post = await entry.render()
  if (post != null) {
    Content = post.Content
  }
}
 
---


<article>
  <Content />
</article>
